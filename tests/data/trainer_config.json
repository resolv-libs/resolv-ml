{
  "output_dir": {
    "path": "./output/trainer/runs",
    "timestamp_format": ""
  },
  "compile": {
    "optimizer": {
      "class_name": "adam",
      "config": {
        "learning_rate": {
            "initial_learning_rate": 0.001,
            "decay_rate": 0.9999,
            "decay_steps": 1,
            "staircase": false
        },
        "beta_1": 0.9,
        "beta_2": 0.999,
        "epsilon": 1e-7,
        "amsgrad": false,
        "weight_decay": null,
        "clipnorm": null,
        "clipvalue": null,
        "global_clipnorm": null,
        "use_ema": false,
        "ema_momentum": 0.99,
        "ema_overwrite_frequency": null
      }
    },
    "loss_weights": null,
    "run_eagerly": true,
    "steps_per_execution": 1,
    "jit_compile": "auto",
    "auto_scale_loss": true
  },
  "fit": {
    "batch_size": 32,
    "epochs": 2,
    "verbose": "auto",
    "shuffle": false,
    "initial_epoch": 0,
    "steps_per_epoch": null,
    "validation_steps": null,
    "validation_batch_size": null,
    "validation_freq": 1
  },
  "callbacks": {
    "model_checkpoint": {
      "filepath": "checkpoints/epoch_{epoch:02d}-val_loss_{val_loss:.2f}.keras",
      "monitor": "val_loss",
      "verbose": 1,
      "save_best_only": true,
      "save_weights_only": false,
      "mode": "auto",
      "save_freq": "epoch",
      "initial_value_threshold": null
    },
    "tensorboard": {
      "log_dir": "tensorboard",
      "histogram_freq": 1,
      "write_graph": true,
      "write_images": false,
      "write_steps_per_second": false,
      "update_freq": "epoch",
      "profile_batch": [500,520],
      "embeddings_freq": 0,
      "embeddings_metadata": null
    },
    "terminate_on_nan": true,
    "csv_logger": {
      "filename": "training_log.csv",
      "separator": ",",
      "append": false
    }
  }
}